<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.dk.sample.api.app.banner.mapper.BannerDAO">

    <select id="getBannerList" resultType="kr.co.dk.sample.api.common.util.LowerKeyMap">
        SELECT *
        FROM momo.banner
        WHERE banner_status = 0
        ORDER BY banner_order ASC, banner_idx DESC
    </select>

</mapper>
